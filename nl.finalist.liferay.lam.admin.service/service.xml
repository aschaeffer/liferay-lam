<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="nl.finalist.liferay.lam.admin.service">
<namespace>LAM</namespace>
    <entity name="Changelog" remote-service="false" local-service="true" cache-enabled="false" json-enabled="false" table="LAM_Changelog">
        <column name="installed_rank" type="int" primary="true"></column>
        <column name="version" type="int"></column>
        <column name="description" type="String"></column>
        <column name="type" type="String" db-name="type"></column>
        <column name="checksum" type="int"></column>
        <column name="installed_by" type="String"></column>
        <column name="installed_on" type="Date"></column>
        <column name="execution_time" type="int"></column>
        <column name="success" type="boolean"></column>
    </entity>
</service-builder>